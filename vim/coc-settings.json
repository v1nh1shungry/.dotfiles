{
    "coc.preferences.enableMessageDialog": true,
    "coc.preferences.formatOnSaveFiletypes": [
        "c",
        "cpp",
        "json",
        "rust",
        "zig"
    ],
    "diagnostic.checkCurrentLine": true,
    "diagnostic.errorSign": " ",
    "diagnostic.hintSign": "",
    "diagnostic.refreshOnInsertMode": true,
    "inlayHint.refreshOnInsertMode": true,
    "notification.statusLineProgress": false,
    "semanticTokens.enable": true,
    "suggest.completionItemKindLabels": {
        "keyword": "\uf1de",
        "variable": "\ue79b",
        "value": "\uf89f",
        "operator": "\u03a8",
        "constructor": "\uf0ad",
        "function": "\u0192",
        "reference": "\ufa46",
        "constant": "\uf8fe",
        "method": "\uf09a",
        "struct": "\ufb44",
        "class": "\uf0e8",
        "interface": "\uf417",
        "text": "\ue612",
        "enum": "\uf435",
        "enumMember": "\uf02b",
        "module": "\uf40d",
        "color": "\ue22b",
        "property": "\ue624",
        "field": "\uf9be",
        "unit": "\uf475",
        "event": "\ufacd",
        "file": "\uf723",
        "folder": "\uf114",
        "snippet": "\ue60b",
        "typeParameter": "\uf728",
        "default": "\uf29c"
    },
    "suggest.noselect": true,
    "clangd.arguments": [
        "--compile-commands-dir=build",
        "--all-scopes-completion",
        "--background-index",
        "--clang-tidy",
        "--completion-style=detailed",
        "--fallback-style=Google",
        "--folding-ranges",
        "--function-arg-placeholders",
        "--header-insertion=never",
        "--include-cleaner-stdlib",
        "--enable-config",
        "-j=12",
        "--pch-storage=memory"
    ],
    "explorer.file.column.indent.indentLine": true,
    "explorer.icon.enableNerdfont": true,
    "explorer.icon.source": "vim-devicons",
    "explorer.quitOnOpen": true,
    "rust-analyzer.server.path": "~/.rustup/toolchains/nightly-x86_64-unknown-linux-gnu/bin/rust-analyzer",
    "zig.path": "/usr/bin/zls"
}
// vim: filetype=jsonc
